<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

	<comment>SQL</comment>
	
	<entry key="writeBoard">
		INSERT INTO BOARD
		VALUES (SEQ_BOARD.NEXTVAL, ?, ?, DEFAULT, ?, DEFAULT)
		</entry>
	
	<entry key="selectAll">
		SELECT * 
		FROM MEMBER
	</entry>
	
	<entry key="selectByUserId">
		SELECT * 
		FROM MEMBER
		WHERE USERID = ?
	</entry>
	
	<entry key="selectByUserName">
		SELECT * 
		FROM MEMBER
		WHERE USERNAME = ?
	</entry>
	
	<entry key="updateMember">
		UPDATE MEMBER 
		SET USERPWD = ?
		, EMAIL = ?
		, PHONE = ?
		, ADDRESS = ? 
		WHERE USERID = ?
	</entry>
	
	<entry key="deleteMember">
		DELETE 
		FROM MEMBER WHERE USERID = ?
	</entry>
	
	</properties>
